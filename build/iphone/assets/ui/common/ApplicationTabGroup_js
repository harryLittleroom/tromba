function ApplicationTabGroup(i){var o=Ti.UI.createTabGroup(),n=new i[0],e=new i[1];win3=new i[2],win4=new i[3],win5=new i[4];for(var r=[n,e,win3,win4],t=[],a=["Our Story","Recipes","Glossary","Find Tromba"],d=["/images/iphoneImage/icon_03.png","/images/iphoneImage/drinkicon.png","/images/iphoneImage/glossary2_03.png","/images/iphoneImage/locationicon.png"],p=0;r.length>p;p++){var u=Ti.UI.createTab({title:a[p],window:r[p],icon:d[p],visible:!1});r[p].containingTab=u,t.push(u)}for(var w=0;4>w;w++)o.addTab(t[w]),Ti.App.TabGroup=o;return o.tabs[1].active=!0,o}module.exports=ApplicationTabGroup;