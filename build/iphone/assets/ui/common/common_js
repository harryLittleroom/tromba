var platformWidth=Ti.Platform.displayCaps.platformWidth,platformHeight=Ti.Platform.displayCaps.platformHeight,debugMode=!1,getImageByFileName=function(i){imageFile=Ti.Filesystem.getFile(Ti.Filesystem.resourcesDirectory,i);var e=imageFile.read();return e},scaleHeightByWidth=function(i){return getImageByFileName(i).height*platformWidth/getImageByFileName(i).width},addBackButton=function(i,e,o){var t=Ti.UI.createButton({height:getImageByFileName("/images/iphoneImage/backButton.png").height/2,width:getImageByFileName("/images/iphoneImage/backButton.png").width/2,backgroundImage:"/images/iphoneImage/backButton.png"});return i.leftNavButton=t,t.addEventListener("click",function(){i.leftNavButton=null,e.animate({top:900,duration:500},function(){i.remove(e)}),o()}),t},addScrollView=function(){var i=Ti.UI.createScrollView({contentWidth:"auto",contentHeight:"auto",showVerticalScrollIndicator:!0,showHorizontalScrollIndicator:!1,height:platformHeight-62.89308176100629-50-10,width:"100%",top:0,zIndex:8});return i},debugSlider=function(i,e,o){var t=Ti.UI.createSlider({top:e,right:0,min:0,max:680,width:"100%",zIndex:100});return t.addEventListener("change",function(i){o(i.source.value)}),debugMode&&i.add(t),t},addTitleBar=function(i,e){var o=Ti.UI.createView({backgroundColor:"#706f73",height:50,width:"auto",top:0,zIndex:9}),t=Ti.UI.createLabel({color:"white",text:i,font:{fontSize:28}});return o.add(t),e.add(o),o},addTableView=function(i,e,o,t){for(var n=[],a=Ti.UI.createTableView({objName:"table",backgroundColor:"black",style:Ti.UI.iPhone.TableViewStyle.PLAIN,separatorStyle:Ti.UI.iPhone.TableViewSeparatorStyle.NONE,separatorColor:"transparent"}),r=0;o.length>r;r++){var d=Ti.UI.createTableViewRow({className:"row",objName:"row",touchEnabled:!0,height:120,backgroundColor:"#212121"});1==r%2&&(d.backgroundColor="#2b2b2b"),o[r]&&d.add(o[r]),t[r]&&d.add(t[r]),n.push(d)}a.setData(n);var u=require("/ui/iphone/drinksListWindow"),l=new u(e);a.addEventListener("click",function(i){i.source&&"table"!==i.source.objName&&e.open(l,{animated:!0})}),i.add(a)};