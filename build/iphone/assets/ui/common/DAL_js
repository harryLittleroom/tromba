var collectionName;exports.getAppData=function(e,i){collectionName=e;var t="http://wf-tromba-staging.littleroominc.com/"+collectionName+"/",o=Ti.Network.createHTTPClient({onload:function(){i(this.responseText)},onerror:function(e){console.log(e.error)},timeout:5e3});o.open("GET",t),o.send()},exports.storeData=function(e){var i=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,"trombaData");i.exists()||i.createDirectory();var t=Ti.Filesystem.getFile(i.resolve(),"data"+collectionName+".js");t.write(e)},exports.readTrombData=function(e){file=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,"trombaData/data"+e+".js");var i=file.read();if(i){var t=JSON.parse(i.text);return t}return alert("Tromba:you must have internet connection for the first time"),void 0},exports.parseHtml=function(e){var i=e.replace(/<li>/g,"îŒ±").replace(/<\/\li>/g,"\n").replace(/<ul>/g,"");return i};