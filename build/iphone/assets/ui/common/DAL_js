var collectionName;exports.getAppData=function(e,i){collectionName=e;var o="http://wf-tromba-staging.littleroominc.com/"+collectionName+"/",t=Ti.Network.createHTTPClient({onload:function(){i(this.responseText)},onerror:function(e){console.log(e.error)},timeout:5e3});t.open("GET",o),t.send()},exports.storeData=function(e){var i=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,"trombaData");i.exists()||i.createDirectory();var o=Ti.Filesystem.getFile(i.resolve(),"data"+collectionName+".js");o.write(e)},exports.readTrombData=function(e){file=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,"trombaData/data"+e+".js");var i=file.read();if(i){var o=JSON.parse(i.text);return o}return alert("Tromba:you must have internet connection for the first time"),void 0},exports.parseHtml=function(e){var i=e.replace(/<li>/g,"îŒ±").replace(/<\/\li>/g,"\n").replace(/<ul>/g,"");return i};