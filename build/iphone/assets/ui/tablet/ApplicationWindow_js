function ApplicationWindow(e){var i=Ti.UI.createWindow({title:e,backgroundColor:"white"}),t=Ti.UI.createButton({height:44,width:200,title:L("openWindow"),top:20});return i.add(t),t.addEventListener("click",function(){i.containingTab.open(Ti.UI.createWindow({title:L("newWindow"),backgroundColor:"white"}))}),i}module.exports=ApplicationWindow;