function drinksListWindow(e){var i=Data.readTrombData("drinks"),t=new commonWindow;t.backgroundColor="#2b2b2b",t.index=1,t.topBarHeight,t.bottomBarHeight;for(var o=getImageByFileName("/images/iphoneImage/bartenderHeaderBackground.jpg"),a=new addScrollView,n="Mrs Eaves OT",r=0;i.length>r;r++){var d=Ti.UI.createView({backgroundColor:"#2b2b2b",width:platformWidth,height:o.height*platformWidth/o.width,sourceID:i[r],index:r}),l=Ti.UI.createView({layout:"vertical",width:"60%",height:Ti.UI.SIZE,right:0,sourceID:i[r]});1==r%2&&(d.backgroundColor="#212121");var c=Ti.UI.createLabel({text:i[r].name,font:{fontSize:22,fontFamily:n},color:"white",height:"auto",width:"auto",index:r,sourceID:i[r],textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT,left:0}),u=Ti.UI.createLabel({text:i[r].bar,font:{fontSize:20,fontFamily:n},color:"white",height:"auto",width:"auto",sourceID:i[r],textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT,left:0,index:r}),m=Ti.UI.createLabel({text:i[r].name,font:{fontSize:18,fontFamily:n},color:"#747474",height:"auto",width:"auto",sourceID:i[r],textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT,left:0,index:r}),p=Ti.UI.createImageView({width:platformWidth/3.5,height:platformWidth/3.5,top:10,left:10,index:r,sourceID:i[r],image:cmsUrl+i[r].drinkfile});l.add(c),l.add(u),l.add(m),d.add(l),d.add(p),debugSlider(t,60,function(e){m.top=e}),debugSlider(t,0,function(e){u.top=e}),d.addEventListener("click",function(i){var t=require("/ui/iphone/individualDrinkWindow"),o=new t(i.source.sourceID);Ti.UI.createWindow({title:"rootWindow",backgroundColor:"blue"}),e.open(o,{animated:!0})}),a.add(d)}return t.add(a),t}var commonWindow=require("ui/iphone/ApplicationWindow"),Data=require("ui/common/DAL");module.exports=drinksListWindow;