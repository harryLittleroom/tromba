function drinksListWindow(e){var i=Data.readTrombData("drinks"),t=new commonWindow;t.backgroundColor="#2b2b2b",t.index=1,t.topBarHeight,t.bottomBarHeight;for(var o=getImageByFileName("/images/iphoneImage/bartenderHeaderBackground.jpg"),a=new addScrollView,n=0;i.length>n;n++){var r=Ti.UI.createView({backgroundColor:"#2b2b2b",width:platformWidth,height:o.height*platformWidth/o.width,sourceID:i[n],index:n});1==n%2&&(r.backgroundColor="#212121");var d=14,l=149,c=Ti.UI.createLabel({text:i[n].name,font:{fontSize:d},color:"white",top:18,height:"auto",width:"auto",index:n,sourceID:i[n],left:l}),u=Ti.UI.createLabel({text:i[n].bar,font:{fontSize:d},color:"white",top:46,height:"auto",width:"auto",left:l,sourceID:i[n],index:n}),m=Ti.UI.createLabel({text:i[n].name,font:{fontSize:d},color:"white",top:75,height:"auto",width:"auto",left:l,sourceID:i[n],index:n}),p=Ti.UI.createImageView({width:platformWidth/3.5,height:platformWidth/3.5,top:10,left:10,index:n,sourceID:i[n],image:cmsUrl+i[n].drinkfile});r.add(c),r.add(u),r.add(m),r.add(p),debugSlider(t,60,function(e){m.top=e}),debugSlider(t,0,function(e){u.top=e}),r.addEventListener("click",function(i){var t=require("/ui/iphone/individualDrinkWindow"),o=new t(i.source.sourceID);Ti.UI.createWindow({title:"rootWindow",backgroundColor:"blue"}),e.open(o,{animated:!0})}),a.add(r)}return t.add(a),t}var commonWindow=require("ui/iphone/ApplicationWindow"),Data=require("ui/common/DAL");module.exports=drinksListWindow;