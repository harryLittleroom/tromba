function searchWindow(){var e=new commonWindow;e.index=3;var t=Ti.UI.createTextField({width:.8*platformWidth,height:30,top:20,borderRadius:10,hintText:"Enter Keywords",backgroundColor:"white",paddingLeft:10,autocorrect:!1,clearButtonMode:Ti.UI.INPUT_BUTTONMODE_ONFOCUS}),i=require("ui/common/DAL"),o=i.readTrombData(),a=getImageByFileName("/images/iphoneImage/bartenderHeaderBackground.jpg"),n=function(){return Ti.UI.createScrollView({backgroundColor:"transparent",width:platformWidth,height:platformHeight-62.89308176100629-50-10-60,top:60})},r=14,d=149;return t.addEventListener("change",function(i){var l=[];m&&e.remove(m);for(var c=0;e.children.length>c;c++)-1!=(""+e.children[c]).search("TiUIScrollView")&&e.remove(e.children[c]);for(var m=new n,g=0;o.length>g;g++){var u=JSON.stringify(o[g]).search(RegExp(i.source.value,"i"));-1!=u&&l.push(o[g])}for(var p=0;l.length>p;p++){var h=Ti.UI.createLabel({text:l[p].contact.name,font:{fontSize:r},color:"white",top:21,height:"auto",width:"auto",left:d,sourceID:l[p]}),w=Ti.UI.createLabel({text:l[p].company.name,font:{fontSize:r},color:"#c9823a",top:49,height:"auto",width:"auto",left:d,sourceID:l[p]}),s=Ti.UI.createLabel({text:l[p].name,font:{fontSize:r},color:"#01b6fd",top:78,height:"auto",width:"auto",left:d,sourceID:l[p]}),f=Ti.UI.createImageView({width:platformWidth/3.5,height:platformWidth/3.5,top:10,left:10,borderRadius:20,image:l[p].contact.picture,sourceID:l[p]}),b=Ti.UI.createView({backgroundImage:"/images/iphoneImage/bartenderHeaderBackground.jpg",width:platformWidth,height:a.height*platformWidth/a.width,top:a.height*platformWidth/a.width*p,index:p,sourceID:l[p]});b.add(h),b.add(w),b.add(s),b.add(f),m.add(b),b.addEventListener("click",function(i){e.remove(m),e.remove(t);var o=require("ui/iphone/bartenderView"),o=new o(i.source.sourceID),a=new addScrollView;new addBackButton(e,a,function(){e.add(m),e.add(t)}),a.zIndex=9,a.add(o),e.add(a)})}e.add(m)}),e.add(t),e}var commonWindow=require("ui/iphone/ApplicationWindow");module.exports=searchWindow;