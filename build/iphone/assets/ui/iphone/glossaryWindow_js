function glossaryWindow(){var e=Data.readTrombData("glossary"),i=new commonWindow,t=["look like a d","look like a o","look like a long o","look like a small o"],o=Ti.UI.createLabel({top:0,text:t[0],color:"white"});i.add(o);for(var a=Ti.UI.createPickerColumn(),n=0,r=e.length;r>n;n++){var d=Ti.UI.createPickerRow(),l=Ti.UI.createLabel({color:"black",font:{fontSize:20,fontWeight:"bold"},text:e[n].term,textAlign:"left",height:"auto",width:"auto"});d.add(l),a.addRow(d)}var c=Ti.UI.createPicker({bottom:0,columns:[a],selectionIndicator:!0});return c.addEventListener("change",function(i){o.text=e[i.rowIndex].description}),i.add(c),i}var commonWindow=require("ui/iphone/ApplicationWindow"),Data=require("ui/common/DAL");module.exports=glossaryWindow;