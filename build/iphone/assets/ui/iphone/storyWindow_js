function storyWindow(){var e=Data.readTrombData("story");console.log(e.description);var i=new commonWindow;i.index=0,i.backgroundColor="#2b2b2b";var t=new addScrollView;t.width=platformWidth;var o=addTitleBar("The Tromba Story",i),a=Ti.UI.createImageView({image:"/images/iphoneImage/our_story_topimage.png",top:o.height,height:"auto",width:platformWidth,zIndex:9}),n=Ti.UI.createLabel({text:e[0].description,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT,left:10,color:"white",font:{fontSize:18,fontFamily:"Mrs Eaves OT"}});return t.add(a),t.add(n),i.add(t),i}var commonWindow=require("ui/iphone/ApplicationWindow"),Data=require("ui/common/DAL");module.exports=storyWindow;