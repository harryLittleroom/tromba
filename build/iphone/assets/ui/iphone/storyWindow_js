function storyWindow(){var e=Data.readTrombData("story"),t=new commonWindow;t.index=0,t.backgroundColor="#2b2b2b";var i=new addScrollView;i.width=platformWidth;var o=addTitleBar("The Tromba Story",t),a=Ti.UI.createImageView({image:"/images/iphoneImage/our_story_topimage.png",top:o.height,height:"auto",width:platformWidth,zIndex:9}),n=Ti.UI.createLabel({text:e[0].description,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT,left:10,color:"white",font:{fontSize:18,fontFamily:"Mrs Eaves OT"}});return i.add(a),i.add(n),t.add(i),t}var commonWindow=require("ui/iphone/ApplicationWindow"),Data=require("ui/common/DAL");module.exports=storyWindow;